@model IEnumerable<MenuDart.Models.TempMenu>

@{
    ViewBag.Title = "Index";
}

<h2>Index (@Model.Count())</h2>

<p>
    @Html.ActionLink("Delete All Older Than 48 Hours", "DeleteAll")
</p>
<table>
    <tr>
        <th>
            DateCreated
        </th>
        <th>
            MenuId
        </th>
        <th>
            SessionId
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DateCreated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MenuId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SessionId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
